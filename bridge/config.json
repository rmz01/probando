{
    "serial": {
        "port": "/dev/ttyUSB0",
        "baudrate": 115200,
        "timeout": 1
    },
    "server": {
        "base_url": "http://localhost:20001",
        "endpoints": {
            "telemetry": "/api/telemetry",
            "system": "/api/telemetry/system",
            "power": "/api/telemetry/power",
            "temperature": "/api/telemetry/temperature",
            "comms": "/api/telemetry/comms"
        },
        "timeout": 5
    }
}
